{
  "name": "@dbsp/core",
  "version": "0.1.0",
  "description": "DBSP: Database Stream Processor - Incremental view maintenance for JavaScript/TypeScript",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./react": {
      "source": "./src/react/index.ts",
      "import": "./dist/react/index.js",
      "types": "./dist/react/index.d.ts"
    },
    "./sql": {
      "source": "./src/sql/index.ts",
      "import": "./dist/sql/index.js",
      "types": "./dist/sql/index.d.ts"
    },
    "./joins": {
      "source": "./src/joins/index.ts",
      "import": "./dist/joins/index.js",
      "types": "./dist/joins/index.d.ts"
    },
    "./internals": {
      "source": "./src/internals/index.ts",
      "import": "./dist/internals/index.js",
      "types": "./dist/internals/index.d.ts"
    },
    "./server": {
      "source": "./src/server/index.ts",
      "import": "./dist/server/index.js",
      "types": "./dist/server/index.d.ts"
    },
    "./core": {
      "source": "./src/core/index.ts",
      "import": "./dist/core/index.js",
      "types": "./dist/core/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "react": [
        "./src/react/index.ts",
        "./dist/react/index.d.ts"
      ],
      "sql": [
        "./src/sql/index.ts",
        "./dist/sql/index.d.ts"
      ],
      "joins": [
        "./src/joins/index.ts",
        "./dist/joins/index.d.ts"
      ],
      "internals": [
        "./src/internals/index.ts",
        "./dist/internals/index.d.ts"
      ],
      "server": [
        "./src/server/index.ts",
        "./dist/server/index.d.ts"
      ],
      "core": [
        "./src/core/index.ts",
        "./dist/core/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc",
    "docs:site": "cd docs && npm run build",
    "docs:dev": "cd docs && npm run dev",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "dbsp",
    "incremental",
    "streaming",
    "sql",
    "react",
    "zset",
    "dataflow",
    "view-maintenance"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "peerDependencies": {
    "react": ">=17.0.0",
    "ws": ">=8.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "ws": {
      "optional": true
    }
  },
  "dependencies": {
    "node-sql-parser": "^5.3.13"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false,
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/ws": "^8.5.0",
    "react": "^18.0.0",
    "rimraf": "^5.0.0",
    "typedoc": "^0.25.0",
    "typedoc-plugin-markdown": "^3.17.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0",
    "ws": "^8.0.0"
  }
}
